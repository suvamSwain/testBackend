{
    "builds":[
        {
        "src" : "backend/wsgi.py",  
        "use" : "@vercel/python",
        "config" : {"maxLambdaSize":"15mb"}
        },
        {
            "src":"build_.sh",
            "use" : "@vercel/static-build"
        }
            ],
    "routes":[{
        "src" : "/(.*)",
        "dest" : "backend/wsgi.py"
    }
    ]
}