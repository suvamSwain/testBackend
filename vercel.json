{
    "version" : 2,
    "builds":[
        {
        "src" : "backend/wsgi.py",  
        "use" : "@vercel/python",
        "config" : {"maxLambdaSize":"15mb", "python_version" : "3.9"}
        }
            ],
    "routes":[{
        "src" : "/(.*)",
        "dest" : "backend/wsgi.py"
    }
    ]
}