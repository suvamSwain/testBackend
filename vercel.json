{
    "version" : 2,
    "builds":[
        {
        "src" : "backend/wsgi.py",  
        "use" : "@vercel/python",
        "config" : {"maxLambdaSize":"15mb", "runtime": "python3.10"}
        },
        {
            "src":"build_.sh",
            "use" : "@vercel/static-build",
            "config":{
                "distDir" : "staticfiles_build"
            }
        }
            ],
    "routes":[{
        "src" : "/(.*)",
        "dest" : "backend/wsgi.py"
    }
    ]
}